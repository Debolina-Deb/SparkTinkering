{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "spark-tinker-lb"
		},
		"BlobSource_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'BlobSource'"
		},
		"SqlSink_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'SqlSink'"
		},
		"Inputblob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Inputblob'"
		},
		"OutputDB_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'OutputDB'"
		},
		"SqlSink_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "tinker-db.database.windows.net"
		},
		"SqlSink_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "spark-tinker-db"
		},
		"SqlSink_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "admin_dd"
		},
		"OutputDB_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "tinker-db.database.windows.net"
		},
		"OutputDB_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "spark-tinker-db"
		},
		"OutputDB_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "admin_dd"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "spark-tinker-lb_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"BlobSource_connectionString": {
						"value": "[parameters('BlobSource_connectionString')]"
					},
					"SqlSink_password": {
						"value": "[parameters('SqlSink_password')]"
					},
					"Inputblob_connectionString": {
						"value": "[parameters('Inputblob_connectionString')]"
					},
					"OutputDB_password": {
						"value": "[parameters('OutputDB_password')]"
					},
					"SqlSink_properties_typeProperties_server": {
						"value": "[parameters('SqlSink_properties_typeProperties_server')]"
					},
					"SqlSink_properties_typeProperties_database": {
						"value": "[parameters('SqlSink_properties_typeProperties_database')]"
					},
					"SqlSink_properties_typeProperties_userName": {
						"value": "[parameters('SqlSink_properties_typeProperties_userName')]"
					},
					"OutputDB_properties_typeProperties_server": {
						"value": "[parameters('OutputDB_properties_typeProperties_server')]"
					},
					"OutputDB_properties_typeProperties_database": {
						"value": "[parameters('OutputDB_properties_typeProperties_database')]"
					},
					"OutputDB_properties_typeProperties_userName": {
						"value": "[parameters('OutputDB_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}